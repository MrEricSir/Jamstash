{
	"manifest_version": 2,
	"name": "MiniSub (Beta)",
	"description": "MiniSub - HTML5 Mini Player for Subsonic",
	"version": "3.0.0",
	"app": {
		"urls": [
			"http://*/*", 
			"https://*/*"
		],
		"launch": {
			"web_url": "http://tsquillario.github.io/MiniSub"
		}
	},
	"icons": {
		"16": "icon_16.png",
		"128": "icon_128.png"
	},
	"permissions": [
		"unlimitedStorage",
		"notifications"
	]
}